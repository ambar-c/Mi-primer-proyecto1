/*style.ccs*/

body {
    font-family: Arial, Helvetica, sans-serif;
    margin: 0;
    display: flex;
    height: 100vh;
}

.container {
    display: flex;
    width: 100%;
}

.sidebar {
    background-color: blue;
    color white;
    width: 250px;
    padding: 20px;
    display: flex;
    flex-direction: column;
    align-items: center;

}

.sidebar .logo {
    text-align: center;
    margin-bottom: 30px;
}

.sidebar .logo img {
    width: 100px;
    margin-bottom: 10px;
}